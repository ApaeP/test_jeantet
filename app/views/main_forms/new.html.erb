<div id="question-card">
  <%= render 'question_card', question: @question, main_form: @main_form %>
  <div class="form-container">
    <%= simple_form_for MainForm.new, remote: true do |f| %>
      <div class="form-fields">
        <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
        <%= f.input :answers, label: "", collection: {yes: "a_yes_b", no: "a_no_endB" }, as: :radio_buttons %>
      </div>
    <% end %>
  </div>
</div>

